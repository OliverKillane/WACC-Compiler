mod ../../../../../stdlib/array_utils.wacc;

begin
	int[] arr = [] ;
	int index = call binary_search_int_arr(1, arr) ;
	println index ;
	arr = [-1] ;
	index = call binary_search_int_arr(0, arr) ;
	println index ;
	arr = [2, 3] ;
	index = call binary_search_int_arr(-2, arr) ;
	println index ;
	arr = [-1, 0, 2] ;
	index = call binary_search_int_arr(3, arr) ;
	println index ;
	arr = [-3, 0, 0, 2] ;
	index = call binary_search_int_arr(-2, arr) ;
	println index ;
	arr = [-3, 1, 2, 3, 3] ;
	index = call binary_search_int_arr(-1, arr) ;
	println index ;
	arr = [-3, -3, -2, 1, 1, 3] ;
	index = call binary_search_int_arr(3, arr) ;
	println index ;
	arr = [-2, -1, 0, 0, 1, 1, 1] ;
	index = call binary_search_int_arr(3, arr) ;
	println index ;
	arr = [-3, -3, -2, -2, -1, 1, 2, 3] ;
	index = call binary_search_int_arr(3, arr) ;
	println index ;
	arr = [-3, -3, -3, -2, -1, 0, 0, 1, 3] ;
	index = call binary_search_int_arr(-3, arr) ;
	println index ;
	arr = [-3, -3, -3, -2, 1, 1, 1, 1, 2, 3] ;
	index = call binary_search_int_arr(-3, arr) ;
	println index ;
