	println index ;
	arr = [-3, -3, -2, -2, -1, -1, 1, 1, 1, 1, 1, 3, 3] ;
	index = call binary_search_int_arr(0, arr) ;
	println index ;
	arr = [-3, -2, -2, -2, -1, -1, -1, 0, 0, 1, 1, 1, 2, 3] ;
	index = call binary_search_int_arr(1, arr) ;
	println index
end