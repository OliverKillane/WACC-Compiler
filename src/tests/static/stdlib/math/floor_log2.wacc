# corectness of floor_log2()

# Output:
# 0
# 1
# 2
# 3
# 3
# 4
# 4
# 6
# 7
# 12
# 17

# Exit:
# 255

mod ../../../../../stdlib/math.wacc;

begin
    # +ve numbers
    int test = call floor_log2(1) ;
    println test ;
    test = call floor_log2(2) ;
    println test ;
    test = call floor_log2(4) ;
    println test ;
    test = call floor_log2(9) ;
    println test ;
    test = call floor_log2(15) ;
    println test ;
    test = call floor_log2(16) ;
    println test ;
    test = call floor_log2(17) ;
    println test ;
    test = call floor_log2(100) ;
    println test ;
    test = call floor_log2(144) ;
    println test ;
    test = call floor_log2(4761) ;
    println test ;
    test = call floor_log2(176401) ;
    println test ;
    
    # -ve exponent
    test = call floor_log2(- 10) ;
    # Should exit before reaching this
    println test 

end