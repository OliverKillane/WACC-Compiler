# corectness of min()

# Output:
# 3
# -1
# 21
# -21
# -12
# -4
# 0
# 0

mod ../../../../../stdlib/math.wacc;

begin
    int test = call min(3, 5) ;
    println test ;
    test = call min(-1, 12) ;
    println test ;
    test = call min(21, 21) ;
    println test ;
    test = call min(-21, -21) ;
    println test ;
    test = call min(-12, -5) ;
    println test ;
    test = call min(-4, 0) ;
    println test ;
    test = call min(10, 0) ;
    println test ;
    test = call min(0, 0) ;
    println test
end